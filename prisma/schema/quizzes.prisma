model Quizzes {
    id String @id @default(uuid())
    guide Guides @relation(fields: [guideId], references: [id])
    guideId String @map("guide_id")
    user Users @relation(fields: [userId], references: [id])
    userId String @map("user_id")
    title String
    description String
    body String
    created_ts DateTime @default(now())
    updated_ts DateTime @default(now())

    @@map("quizzes")
    @@unique([guideId,userId])
    
}
